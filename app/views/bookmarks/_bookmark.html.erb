<tr id='row_<%= bookmark.id %>'>
    <td><%= bookmark.name %></td>
    <td><%= bookmark.type.name %></td>
        <% bookmark.categories.each do |item| %>
        <td><%= item.name %></td>
        <% end %>
    <td><%= link_to 'Show', bookmark, remote: true %></td>
    <td><%= link_to 'Edit', edit_bookmark_path(bookmark), remote: true %></td>
    <td><%= link_to 'Destroy', bookmark, remote: true, method: :delete, data: { confirm: 'Seguro de eliminar?' } %></td>
</tr>